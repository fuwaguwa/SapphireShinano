{
  "name": "sapphireshinano",
  "version": "3.0.0",
  "description": "Shinano rewritten to be using SapphireJS",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon dist/index.js",
    "eslint": "eslint \"**/*.ts\" --fix",
    "prettify": "prettier --write \"**/*.ts\"",
    "format": "npm run prettify && npm run eslint",
    "build": "tsc",
    "watch": "tsc -w",
    "watch:start": "tsc-watch --onSuccess \"node ./dist/index.js\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azurapi/azurapi": "^1.1.2",
